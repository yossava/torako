<%= render 'layouts/footer' %>
<% if @stroli3 %>
<% @stroli3.each do |s| %>
<script>
  	$('.ok<%= s.id %>').click(function(){
		var $this = $(this);
		$this.toggleClass('See');
		if($this.hasClass('Sees')){
			$this.text('See');
		} else {
			$this.text('Mohon Tunggu..');
		}
	});
</script>
<script>
  	$('.no<%= s.id %>').click(function(){
		var $this = $(this);
		$this.toggleClass('See');
		if($this.hasClass('Sees')){
			$this.text('See');
		} else {
			$this.text('Mohon Tunggu..');
		}
	});
</script>
<% end %>
