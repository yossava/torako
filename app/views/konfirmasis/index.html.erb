<p id="notice"><%= notice %></p>

<h1>Listing Konfirmasis</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Cart</th>
      <th>Tanggal</th>
      <th>Bulan</th>
      <th>Tahun</th>
      <th>Metode</th>
      <th>Dari</th>
      <th>Tujuan</th>
      <th>Keterangan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @konfirmasis.each do |konfirmasi| %>
      <tr>
        <td><%= konfirmasi.user_id %></td>
        <td><%= konfirmasi.cart_id %></td>
        <td><%= konfirmasi.tanggal %></td>
        <td><%= konfirmasi.bulan %></td>
        <td><%= konfirmasi.tahun %></td>
        <td><%= konfirmasi.metode %></td>
        <td><%= konfirmasi.dari %></td>
        <td><%= konfirmasi.tujuan %></td>
        <td><%= konfirmasi.keterangan %></td>
        <td><%= link_to 'Show', konfirmasi %></td>
        <td><%= link_to 'Edit', edit_konfirmasi_path(konfirmasi) %></td>
        <td><%= link_to 'Destroy', konfirmasi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Konfirmasi', new_konfirmasi_path %>
